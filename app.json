{
  "expo": {
    "name": "antwise-app",
    "slug": "antwise-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "antwiseapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#7C00FE"
      },
      "package": "com.theantwiseapp.antwiseapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#7C00FE"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "9f65f540-78c0-4eb1-aa9e-e38c35b7bd5c"
      }
    },
    "owner": "theantwiseapp",
    "experiments": {
      "typedRoutes": true
    }
  }
}
